{
  "name": "viteproj",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "eslint": "eslint src/ --fix",
    "prettier": "prettier --write \"src/**/*.{js,jsx,json,html,css,md}\"",
    "stylelint": "stylelint \"src/**/*.css\" --fix",
    "lint": "npm run stylelint && npm run eslint && npm run prettier",
    "prepare": "husky",
    "commit": "cz",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.1",
    "@eslint/js": "^9.36.0",
    "@laynezh/vite-plugin-lib-assets": "^2.1.3",
    "@storybook/addon-a11y": "^10.2.13",
    "@storybook/addon-docs": "^10.2.13",
    "@storybook/addon-onboarding": "^10.2.13",
    "@storybook/addon-vitest": "^10.2.13",
    "@storybook/react-vite": "^10.2.13",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-perfectionist": "^5.6.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^10.2.13",
    "eslint-plugin-unused-imports": "^4.4.1",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "playwright": "^1.58.2",
    "prettier": "^3.8.1",
    "prop-types": "^15.8.1",
    "storybook": "^10.2.13",
    "stylelint": "^17.3.0",
    "stylelint-config-standard": "^40.0.0",
    "stylelint-order": "^7.0.1",
    "vite": "^7.1.7",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-image-optimizer": "^2.0.3",
    "vite-plugin-markdown": "^2.0.0-2",
    "vite-plugin-readable-classnames": "^1.5.3",
    "vitest": "^4.0.18"
  },
  "lint-staged": {
    "./src/**/*.css": [
      "stylelint --fix",
      "prettier --write"
    ],
    "./src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{json,html,md}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
